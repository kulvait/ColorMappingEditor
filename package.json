{
  "name": "color-mapping-editor",
  "version": "1.1.0",
  "description": "Fork of the DLR-SC Transfer Function Editor, integrating its D3-based color mapping with React and vtk.js.",
  "authors": [
    "Original by German Aerospace Center (DLR)",
    "Fork by Vojtech Kulvait"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:kulvait/ColorMappingEditor.git"
  },
  "keywords": [
    "react",
    "vtk.js",
    "transfer function",
    "color mapping",
    "opacity",
    "d3",
    "visualization"
  ],
  "type": "module",
  "main": "dist/color-mapping-editor.umd.js",
  "module": "dist/color-mapping-editor.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "pnpm -r dev",
    "build": "vite build",
    "prepare": "pnpm run build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "clean": "rm -rf node_modules dist",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint-fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "test": "vitest run"
  },
  "dependencies": {
    "d3-color": "^3.1.0",
    "d3-hsv": "^0.1.0",
    "d3-interpolate": "^3.0.1",
